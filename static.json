{
  "root": "dist",
  "clean_urls": true,
  "routes": {
    "/jukebox": "/jukebox.html",
    "/admin": "/admin/index.html",
    "/player": "/player.html",
    "/**": "/index.html"
  },
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  "https_only": true,
  "directory_index": "index.html"
}
